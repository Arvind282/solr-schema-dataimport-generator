    "entityQueries":{
        "eindverbuiker":{
            "dataSource": "global",
            "transformers": ["RegexTransformer"],
            "tables": {
                "main": ["FROM `eindverbruiker` ev"],
                "all": ["LEFT JOIN `kenmerken_taal` kt_land ON ev.`land`=kt_land.`code` AND kt_land.`tabel`='land'",
                        "LEFT JOIN `kenmerken_taal` kt_provincie ON ev.`provincie`=kt_provincie.`code` AND kt_provincie.`tabel`='provincie'",
                        "LEFT JOIN `kenmerken_taal` kt_moeder ON ev.`scode`=kt_moeder.`code` AND kt_moeder.`tabel`='moeder'",
                        "LEFT JOIN `kenmerken_taal` kt_scode ON ev.`scode`=kt_scode.`code` AND kt_scode.`tabel`='scode'",
                        "LEFT JOIN `kenmerken_taal` kt_bcode ON ev.`bcode`=kt_bcode.`code` AND kt_bcode.`tabel`='bcode'"
                ]
            },
            "queries": {
                "query": {
                    "filter": "WHERE ev.`status` > 0 GROUP BY ev.`klantnr`",
                    "tables": ["main", "all"]
                },
                "deltaImportQuery": {
                    "filter": "WHERE ev.`klantnr`='${dih.delta.id}' GROUP BY ev.`klantnr`",
                    "tables": ["main", "all"]
                },
                "deltaQuery": {
                    "filter": "WHERE ev.`updatedon` &gt; '${dih.last_index_time}'",
                    "tables": ["main"],
                    "fields": ["id"]
                }
            }
        },
        "eindverbuiker_features":{
            "dataSource": "global",
            "transformers": ["RegexTransformer"],
            "parentEntity": "eindverbuiker",
            "pk": "klantnr",
            "tables": {
                "main": ["FROM `eindverbruiker` ev"],
                "all": ["LEFT JOIN `kenmerken_taal` kt_type ON kt_type.`tabel`='type' AND FIND_IN_SET(LEFT(kt_type.`code`,6),ev.`type`)",
                    "LEFT JOIN `kenmerken_tags` kt01 ON kt01.`kenmerk01`=ev.`kenmerk01`",
                    "LEFT JOIN `kenmerken_tags` kt03 ON kt03.`kenmerk03`=ev.`kenmerk03`",
                    "LEFT JOIN `kenmerken_tags` kt04 ON kt04.`kenmerk04`=ev.`kenmerk04`",
                    "LEFT JOIN `kenmerken_tags` kt05 ON kt05.`kenmerk05`=ev.`kenmerk05`",
                    "LEFT JOIN `kenmerken_tags` kt06 ON kt06.`kenmerk06`=ev.`kenmerk06`",
                    "LEFT JOIN `kenmerken_tags` kt07 ON kt07.`kenmerk07`=ev.`kenmerk07`",
                    "LEFT JOIN `kenmerken_tags` kt09 ON kt09.`kenmerk09`=ev.`kenmerk09`",
                    "LEFT JOIN `kenmerken_tags` kt11 ON FIND_IN_SET(kt11.`kenmerk11`,ev.`kenmerk11`)",
                    "LEFT JOIN `kenmerken_tags` kt41 ON kt41.`kenmerk41`=ev.`kenmerk41`",
                    "LEFT JOIN `kenmerken_tags` kt43 ON kt43.`kenmerk43`=ev.`kenmerk43`",
                    "LEFT JOIN `kenmerken_tags` kt71 ON kt71.`kenmerk71`=ev.`kenmerk71`",
                    "LEFT JOIN `kenmerken_tags` kt92 ON kt92.`kenmerk92`=ev.`kenmerk92`",
                    "LEFT JOIN `kenmerken_tags` kt95 ON kt95.`kenmerk95`=ev.`kenmerk95`",
                    "LEFT JOIN `kenmerken_tags` kt96 ON FIND_IN_SET(kt96.`kenmerk96`,ev.`kenmerk96`)",
                    "LEFT JOIN `kenmerken_tags` kt97 ON FIND_IN_SET(kt97.`kenmerk97`,ev.`kenmerk97`)"
                ]
            },
            "queries": {
                "query": {
                    "filter": "WHERE ev.`klantnr`= '${eindverbuiker.klantnr}'",
                    "tables": ["main", "all"]
                }
            }
        },
        "surrounding":{
            "dataSource": "datawarehouse",
            "parentEntity": "eindverbuiker",
            "pk": "klantnr",
            "tables": {
                "main": ["FROM `ev_surrounding` evs"]
            },
            "queries":{
                "query": {
                    "filter": "WHERE evs.`klantnr`= '${eindverbuiker.klantnr}' AND evs.`radius` = '0.4'",
                    "tables": ["main"]
                }
            }
        }
    },
